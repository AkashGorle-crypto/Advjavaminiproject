<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveServlet2</servlet-name>
  	<display-name>SaveServlet2</display-name>
  	<description></description>
  	<servlet-class>com.servlets.SaveServlet2</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteServlet2</servlet-name>
  	<display-name>DeleteServlet2</display-name>
  	<description></description>
  	<servlet-class>com.servlets.DeleteServlet2</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateServlet2</servlet-name>
  	<display-name>UpdateServlet2</display-name>
  	<description></description>
  	<servlet-class>com.servlets.UpdateServlet2</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>SaveServlet2</servlet-name>
  	<url-pattern>/SaveServlet2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServlet2</servlet-name>
  	<url-pattern>/DeleteServlet2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServlet2</servlet-name>
  	<url-pattern>/UpdateServlet2</url-pattern>
  </servlet-mapping>
  
</web-app>
